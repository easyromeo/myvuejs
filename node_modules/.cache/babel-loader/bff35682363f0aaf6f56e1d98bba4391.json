{"ast":null,"code":"// import dates from './test1/dates';\n// import TestList from './components/TestList.vue';\n\nexport default {\n  name: 'App',\n  // components: { TestList },\n  component: ('task-list', {\n    props: ['tasks'],\n    template: `\n    <div class=\"list\">\n    <div class=\"item\" :class=\"{done: task.done}\" v-for=\"task in tasks\" :key=\"task.text\">\n      <input type=\"checkbox\" v-model=\"task.done\">\n      {{ task.text }}\n    </div>\n  </div>\n    `\n  }),\n  data() {\n    return {\n      // dates,\n      tasks: [{\n        text: 'White',\n        done: true\n      }, {\n        text: 'Black',\n        done: false\n      }, {\n        text: 'Pink',\n        done: false\n      }, {\n        text: 'Blue',\n        done: false\n      }, {\n        text: 'Red',\n        done: false\n      }]\n    };\n  },\n  methods: {\n    completedTasks() {\n      return this.tasks.filter(task => task.done);\n    },\n    uncompletedTasks() {\n      return this.tasks.filter(task => !task.done);\n    }\n  }\n};","map":{"version":3,"mappings":"AA4BA;AACA;;AAEA;EACAA;EACA;EACAC;IACAC;IACAC;AACA;AACA;AACA;AACA;AACA;AACA;AACA;EACA;EACAC;IACA;MACA;MACAC,QACA;QAAAC;QAAAC;MAAA,GACA;QAAAD;QAAAC;MAAA,GACA;QAAAD;QAAAC;MAAA,GACA;QAAAD;QAAAC;MAAA,GACA;QAAAD;QAAAC;MAAA;IAEA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","component","props","template","data","tasks","text","done","methods","completedTasks","uncompletedTasks"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<!-- eslint-disable vuejs-accessibility/alt-text -->\n<!-- eslint-disable vue/no-multiple-template-root -->\n<!-- eslint-disable vue/valid-template-root -->\n<template>\n  <h1>Список задач <div>\n      <like-button></like-button>\n      <dislike-button></dislike-button>\n    </div>\n  </h1>\n  <div v-if=\"count() == 0\">Вы великолепны!</div>\n  <div v-else-if=\"count() == 1\"> Остался последний рывок!</div>\n  <div v-else>Осталось сделать задач: <span class=\"counter\">{{ count() }}</span></div>\n  <task-list :tasks=\"uncompletedTasks()\"></task-list>\n  <div class=\"form\">\n    <input v-model=\"message\">\n    <button type=\"button\" @click=\"addTask\">Добавить</button>\n    <like-button type=\"button\">&#9829;</like-button>\n    <dislike-button></dislike-button>\n  </div>\n  <transition name=\"bounce\">\n    <img src=\"https://pbs.twimg.com/media/BG2zfl6CEAEWL3B.jpg\" v-show=\"count() == 0\">\n  </transition>\n\n  <task-list :tasks=\"completedTasks()\"></task-list>\n\n</template>\n\n<script>\n// import dates from './test1/dates';\n// import TestList from './components/TestList.vue';\n\nexport default {\n  name: 'App',\n  // components: { TestList },\n  component: ('task-list', {\n    props: ['tasks'],\n    template: `\n    <div class=\"list\">\n    <div class=\"item\" :class=\"{done: task.done}\" v-for=\"task in tasks\" :key=\"task.text\">\n      <input type=\"checkbox\" v-model=\"task.done\">\n      {{ task.text }}\n    </div>\n  </div>\n    `,\n  }),\n  data() {\n    return {\n      // dates,\n      tasks: [\n        { text: 'White', done: true },\n        { text: 'Black', done: false },\n        { text: 'Pink', done: false },\n        { text: 'Blue', done: false },\n        { text: 'Red', done: false },\n      ],\n    };\n  },\n  methods: {\n    completedTasks() {\n      return this.tasks.filter((task) => task.done);\n    },\n    uncompletedTasks() {\n      return this.tasks.filter((task) => !task.done);\n    },\n  },\n};\n</script>\n\n<style lang=\"stylus\">\n\n</style>\n"]},"metadata":{},"sourceType":"module"}